  @prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#> .
  @base <http://arida.ufc.br/meta-ekg/resource> .
<#DB_source> a d2rq:Database;
    d2rq:jdbcDSN "jdbc:postgresql://localhost:5432/postgres";
    d2rq:jdbcDriver "org.postgresql.Driver";
    d2rq:username "postgres";
    d2rq:password "123456".

<#LogicalSource> a rml:LogicalSource;
    rml:source <#DB_source>;
    rr:sqlVersion rr:SQL2008;
    rr:tableName ""; 
    rml:query """SELECT * FROM "UFC2"."RFB_ESTAB";""";
    rml:referenceFormulation ql:CSV.

<#MyTriplesMap> a rr:TriplesMap;
    rml:logicalSource <#LogicalSource>;
    rr:subjectMap [
        rr:template "http://www.sefaz.ma.gov.br/resource/RFB/Estabelecimento/{CNPJ}";
        rr:class <http://www.sefaz.ma.gov.br/RFB/ontology/Estabelecimento>
    ];
    rr:predicateObjectMap [
       rr:predicate <http://www.sefaz.ma.gov.br/CADSEFAZ/ontology/cnpj> ;
       rr:objectMap [ rml:reference "CNPJ" ]
    ];
	rr:predicateObjectMap [
       rr:predicate <http://www.sefaz.ma.gov.br/CADSEFAZ/ontology/inscricao_estadual> ;
       rr:objectMap [ rml:reference "INSCRICAO_ESTADUAL" ]
    ];
	rr:predicateObjectMap [
       rr:predicate <http://www.sefaz.ma.gov.br/CADSEFAZ/ontology/razao_social> ;
       rr:objectMap [ rml:reference "RAZAO_SOCIAL" ]
    ];
	rr:predicateObjectMap [
       rr:predicate <http://www.w3.org/2000/01/rdf-schema#label> ;
       rr:objectMap [ rml:reference "ROTULO" ]
    ].
    

