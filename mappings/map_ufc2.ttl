@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix : <http://example.org/rules/> .
@prefix schema: <http://schema.org/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix sfz: <http://www.sefaz.ma.org/ontology/> .

@base <http://example.org/rules/> .
  

<TriplesMap1> a rr:TriplesMap;
  rml:logicalSource [
    rml:source <#DB_source>;
    rr:sqlVersion rr:SQL2008;
    rr:tableName "\"UFC2\".\"RFB_ESTAB\"";
    rml:query "SELECT * FROM \"UFC2\".\"RFB_ESTAB\"";
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
    rr:template "http://example.com/Estab/{cnpj}";
    rr:class sfz:Estab 
  ];
 

  rr:predicateObjectMap [
    rr:predicate sfz:cnpj ;
    rr:objectMap [ rml:reference "cnpj" ]
  ];
  rr:predicateObjectMap [
    rr:predicate sfz:nome_fantasia ;
    rr:objectMap [ rml:reference "nome_fantasia" ]
  ];
  rr:predicateObjectMap [
    rr:predicate sfz:telefone ;
    rr:objectMap [ rml:reference "telefone" ]
  ]
  .


<#DB_source> a d2rq:Database;
    d2rq:jdbcDSN "jdbc:postgresql://localhost:5432/postgres";
    d2rq:jdbcDriver "org.postgresql.Driver";
    d2rq:username "postgres";
    d2rq:password "123456" .